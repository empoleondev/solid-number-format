    "eslint": "^6.5.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "gh-pages": "^4.0.0",
    "jsdom": "^23.0.0",
