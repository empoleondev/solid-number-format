{
  "name": "@empoleon/solid-number-format",
  "description": "Solidjs component to format number in an input or as a text.",
  "version": "0.0.4",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "author": "Paul M",
  "license": "MIT",
  "keywords": [
    "solid-component",
    "solid",
    "currency",
    "input",
    "number",
    "number-format",
    "mask"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "webpack-dev-server --hot",
    "start-doc": "docusaurus start documentation/v5 --port 9000",
    "build-doc": "docusaurus build documentation/v5",
    "publish-doc": "node publish_doc.js",
    "format": "prettier --write ./src",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:js": "rollup -c --no-sourcemap",
    "build": "yarn clean && yarn build:types && yarn build:js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "test-build": "vitest run",
    "test-ts": "yarn tsc --noEmit",
    "lint": "cross-env eslint src/**",
    "storybook": "storybook dev -p 6006",
    "newchangeset": "yarn exec changeset",
    "version": "yarn changeset version",
    "release": "yarn changeset publish",
    "version:dev": "yarn version prerelease",
    "release:dev": "yarn npm publish --tag dev"
